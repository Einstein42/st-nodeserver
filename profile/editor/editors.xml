<editors>
    <!-- IPSO Digital Input True/False -->
    <editor id="di_tf">
        <range uom="56" subset="0,1" />
    </editor>
    <!-- IPSO Digital Input Raw -->
    <editor id="di_raw">
        <range uom="56" min="0" max="65535" prec="0" />
    </editor>
    <!-- IPSO Temp -->
    <editor id="temp">
        <range uom="4" min="-50" max="80" prec="6" />
    </editor>
    <!-- IPSO Humidity -->
    <editor id="hum">
        <range uom="22" min="1" max="100" prec="6" />
    </editor>
    <!-- IPSO Accelerometer -->
    <editor id="acel">
        <range uom="56" min="-10" max="10" prec="6" />
    </editor>
    <!-- IPSO Magnetometer -->
    <editor id="magn">
        <range uom="56" min="-10" max="10" prec="6" />
    </editor>
    <!-- IPSO Barometer -->
    <editor id="baro">
        <range uom="56" min="260" max="1260" prec="6" />
    </editor>
</editors>
